        <jsFile name="num.js"/>
        <jsFile name="color.js"/>
        <jsFile name="boxmodel.js"/>
        <jsFile name="func.js"/>
        <jsFile name="fx.js"/>
        <jsFile name="base64.js" />
        <jsFile name="Serializer.js" />
        <jsFile name="Xhr.js"/>
      <jsFile name="DynamicClassFactory.js" />
        <jsFile name="Event.js" />
    </jsDir>
    <jsDir path="javascript/mojo/js/source/storage">
        <jsFile name="DOMStorage.js" />
        <jsFile name="DOMLocalStorage.js" />
        <jsFile name="DOMSessionStorage.js" />
    </jsDir>
</jsBundle>    

<jsBundle name="mojo-core-debug" depSort="false" inherits="mojo-core" minify="true" type="comments-only" >
