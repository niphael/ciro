.mstrmojo-Chart-lbl {
	color: #333333;
	font: Tahoma;
	font-size: 8pt;
	font-weight: bold;
	overflow: hidden;
	position: absolute;
	top: 0;	
	width:30px;
	text-align:right;
	-webkit-user-select: none;
	-moz-user-select: -moz-none;
	-khtml-user-select: none;
	-webkit-user-select: none;
	-o-user-select: none;
	user-select: none;
}

.mstrmojo-Chart-tooltip {
	display:block;
	min-width: 80px;
	min-height: 35px;
	position: absolute;
	border-radius: 4px 4px 4px 4px;
	top: 10px;
	background-image: -webkit-gradient(linear, left top, left bottom, from(#fffce9),
		to(#faf4c3) );
	background-image: -moz-linear-gradient(top, #fffce9, #faf4c3);
	-ms-filter:
		"progid:DXImageTransform.Microsoft.gradient(startColorstr=white, endColorstr=gray)"
		;
	color: #111 !important;
	font: Tahoma;
	font-size: 8pt;
	font-weight: normal;
	text-align: center;
	padding: 3px;
	-webkit-box-shadow: 0px 2px 5px #000;
	-moz-box-shadow: 0px 2px 5px #000;
	-ms-box-shadow: 0px 2px 5px #000;
	box-shadow: 0px 2px 5px #000;
	-webkit-border-radius: 2px;
	-moz-border-radius: 2px;
	opacity: 0;
	-webkit-transition: opacity 0.1s linear;
	-ms-transition: opacity 0.1s linear;
	-moz-transition: opacity 0.1s linear;
	-webkit-user-select: none;
	-moz-user-select: -moz-none;
	-khtml-user-select: none;
	-webkit-user-select: none;
	-o-user-select: none;
	user-select: none;
	z-index: 100;
}

.fadeIn {
	opacity: 1;
}

.mstrmojo-Chart {
	-webkit-user-select: none;
	-moz-user-select: -moz-none;
	-khtml-user-select: none;
	-webkit-user-select: none;
	-o-user-select: none;
	user-select: none;
	cursor: pointer;
	border-radius: 0px;
	border-color:blue;
	
