.dimgr-data-tbl {
    width: 98%;
    height: auto;
    margin: 10px;
    margin-bottom: 10px;
}

.dimgr-data-tbl .mstrmojo-DataGrid-itemsScrollBox {
    max-height: 100%;
    overflow-y: auto;
    overflow-x: hidden;
}

.dimgr-create-new-btn {
    width: 220px;
    text-decoration: underline;
    color: blue;
    font-size: 11pt;
    margin-top: 10px;
    margin-bottom: 7px;
    margin-left: 5px;
    margin-right: 5px;
    text-align: left;
}

.dimgr-confirm-box {
    text-align: center;
    color: green;
    font-weight: bold;
    font-style: normal;
    font-variant: normal;
    font-size: 11pt;
    line-height: normal;
    font-family: Tahoma, Arial, Verdana;
    padding-left: 10px;
    padding-top: 10px;
    padding-bottom: 10px;
}

.dimgr-edit-title {
    color: #e6e6e6;
    background: #494949;
    margin: 0px 0px 0px 0px;
    font-weight: bold;
    font-style: normal;
    font-variant: normal;
    font-size: 11pt;
    line-height: normal;
    font-family: Tahoma, Arial, Verdana;
    border: solid 1px #AAA;
    overflow: visible;
    -moz-border-radius: 3px;
    border-radius: 5px;
    padding-left: 5px;
    padding-top: 3px;
    padding-bottom: 5px;
}

.dimgr-edit-err-txt {
    color: red;
    margin: 0px 0px 0px 20px;
    font-style: normal;
    font-variant: normal;
    font-size: 11pt;
    line-height: normal;
    font-family: Tahoma, Arial, Verdana;
    padding-left: 5px;
    padding-top: 3px;
    padding-bottom: 5px;
}

.dimgr-edit-header {
    margin: 0px 0px 0px 0px;
    width: 100%;
}

.dimgr-editor-box {
    margin: 5px 5px 5px 5px;
    overflow: visible;
}

.dimgr-save-btn {
    width: 50px;
    display: block;
    margin-left: auto;
    margin-right: auto;
    padding-left: 2px;
    padding-right: 2px;
    padding-top: 2px;
    padding-bottom: 2px;
}

.dimgr-edit-btn-bar {
    margin: 3px 0px 3px 6px;
    text-align: left;
    width: 15%    
}

.dimgr-field-label {
    margin: 3px 3px 3px 5px;
    font-size: 10pt;
    color: #333333;
} {margin:
    6px 0px 3px 0px;
