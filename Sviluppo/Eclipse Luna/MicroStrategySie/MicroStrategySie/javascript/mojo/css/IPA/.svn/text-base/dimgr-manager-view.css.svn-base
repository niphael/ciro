.dimgr-textbox {
    margin: 3px 3px 3px 3px;
    font-size: 11px;
    font-family: Tahoma, Arial, Verdana;
}

.dimgr-textarea {
    margin: 3px 5px 3px 5px;
    resize: none;
    width: 310px;
    height: 85px;
    border: 2px inset #D4D0C8
}

.dimgr-fieldset {
    margin: 0px 5px 3px 5px;
}

.dimgr-checkbox {
    margin: 6px 0px 3px 0px;
    display: block;
    font-size: 10pt;
    color: #333333;
}


