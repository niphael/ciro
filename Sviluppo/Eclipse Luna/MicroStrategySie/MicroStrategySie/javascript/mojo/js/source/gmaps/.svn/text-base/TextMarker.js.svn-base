TextMarkerScript=true;(function(){mstrmojo.requiresCls("mstrmojo.gmaps.CustomMarker");mstrmojo.gmaps.TextMarker=mstrmojo.declare(mstrmojo.gmaps.CustomMarker,null,{scriptClass:"mstrmojo.gmaps.TextMarker",div_:null,position:null,labelOffset:new google.maps.Size(0,0),labelClass:"textMarker",labelText:"",width:"50px",onAdd:function onAdd(){this._super();var div=this.div_;div.className=this.labelClass;div.innerHTML=this.labelText;},draw:function draw(){var overlayProjection=this.getProjection();var p=overlayProjection.fromLatLngToDivPixel(this.position);this.div_.style.left=(p.x+this.labelOffset.width)+"px";this.div_.style.top=(p.y+this.labelOffset.height)+"px";}});})();