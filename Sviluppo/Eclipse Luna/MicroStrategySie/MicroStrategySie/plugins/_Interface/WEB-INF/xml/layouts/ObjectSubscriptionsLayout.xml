<!DOCTYPE mstrlayout:layout SYSTEM "mstrlayout.dtd">
<mstrlayout:layout>
	<mstrlayout:if name="hasSubscriptionsOrCanSubscribe">
		<mstrlayout:then>
			
			<mstrlayout:if name="shouldShowSubscriptionsList">
				<mstrlayout:argument type="int" value="2"/>
				<mstrlayout:then>
					<mstrlayout:render name="subscriptionsByDeliveryMode">
						<mstrlayout:argument type="int" value="2"/>
					</mstrlayout:render>
				</mstrlayout:then>
			</mstrlayout:if>
			
		</mstrlayout:then>
		<mstrlayout:else>
			<mstrlayout:render name="showMessage"/>
		</mstrlayout:else>
	</mstrlayout:if>
</mstrlayout:layout>