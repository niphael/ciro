<!DOCTYPE beanTask SYSTEM "../dtds/beanTask.dtd">
<beanTask class="com.microstrategy.web.app.tasks.GetRWGraphImageTask">
  <basicInfo desc="This Task returns the binary image data for a graph in a Report Writer document.">
    <parameter name="width" desc="Specifies the width (in pixels) of the graph image." required="true"/>
    <parameter name="height" desc="Specifies the height (in pixels) of the graph image." required="true"/>
    <parameter name="messageID" desc="The Message ID for the previously executed Report Writer document. It is optional if a GraphBean state is provided." required="false"/>
    <parameter name="imgType" desc="The type of image desired. See com.microstrategy.webapi.EnumDSSXMLGraphImgTypes for possible values.  This value is optional if the caller provides a previous GraphBean state." required="false" />
    <parameter name="nodeKey" desc="The key of the template node whose graph should be generated. This value is optional if the caller provides a previous GraphBean state." required="false" />
    <parameter name="sliceID" desc="The slice identifier of the template node whose graph should be generated. This value is optional if the caller provides a previous GraphBean state." required="false" />
  </basicInfo>
  
  <beanInfo name="gb" bean-type="GraphBean" />
</beanTask>
