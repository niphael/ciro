/* waiting box css*/
.mstrmojo-ProgressBox .mstrmojo-Editor-content{
    background: #D8EBFB url(images/gradient_bg_lightblue_short.png) repeat-x scroll 0 0; 
    height: 70px;
    -moz-border-radius-bottomright: 0px;
	-webkit-border-bottom-right-radius: 0px;
	-moz-border-radius-bottomleft: 0px;
	-webkit-border-bottom-left-radius: 0px;
}

.mstrmojo-ProgressBox-Wait {
    background: transparent url(images/preloader_balls.gif) no-repeat scroll center 25px;
    height: 60px;
    width: 60px;
    float: left; 	
}

.mstrmojo-ProgressBox-Message{
	font-family: arial;
    font-size: 12px;
    color: #333333;
    font-weight: bold;
	margin-top: 25px;
}

.mstrmojo-IPA-pulldown-height {
	max-height:200px;
	overflow:auto;
}

.SecurityRoleEditor{
	width:946px; 
	margin-top: 10px;
	border: 1px solid #AAA; 
	padding: 5px;
}

#SecurityRoleTable .mstrmojo-DataGrid-itemsContainer td{
	text-align: left;
}

#SubscriptionManagerDataGrid .mstrmojo-DataGrid-itemsContainer td{
	text-align: left;
}

.SecurityRoleActions{
	width:900px;
	margin-top: 10px;
}

