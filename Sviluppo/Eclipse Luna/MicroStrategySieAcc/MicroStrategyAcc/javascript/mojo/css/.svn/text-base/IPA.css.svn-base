}

.mstrmojo-enviroment-panel {
    margin-left: 10px;
    margin-right: 5px;
    margin-top: 10px;
    margin-bottom: 5px;
    overflow: visible;
    border-radius: 4px 4px 4px 4px; 
	border:1px solid #BABABA;
    -moz-border-radius: 4px;
	//-webkit-box-shadow: -1px 0px 1px 1px #AAA,1px 2px 1px 1px #AAA;
	//-moz-box-shadow: -1px 0px 1px 1px #AAA,1px 2px 1px 1px #AAA;
	//box-shadow: inset 1px 1px 1px 1px #AAA,1px 2px 1px 1px #AAA;
	background-image:#f2f2f2; 
	background-image:-webkit-gradient(linear, left top, left bottom, color-stop(0.5, #fafafa), to(#f2f2f2)); 
	background-image:-moz-linear-gradient(top, #fafafa 50%, #f2f2f2);
	background-image: "linear-gradient(top, #fafafa, #f2f2f2)"; 
	-ms-filter: "progid:DXImageTransform.Microsoft.gradient(startColorStr='#fafafa', EndColorStr='#f2f2f2')"; /* IE8 */
}

.mstrmojo-chart-panel {
    margin-left: 10px;
    margin-right: 5px;
    margin-top: 10px;
    margin-bottom: 5px;
    overflow: visible;
    -moz-border-radius: 1px;
    border-radius: 3px;
	-webkit-box-shadow: -1px 0px 0px 0px #AAA,0px 0px 0px 1px #AAA;
	-moz-box-shadow: -1px 0px 1px 1px #AAA,0px 0px 0px 1px #AAA;
	box-shadow: : -1px 0px 1px 1px #AAA,1px 0px 1px 1px #AAA;
	background:#fafafa; 
}

