body {
    margin: 0;
    background-color: #F5F5F2;
    height: 100%;
    font: 8pt Tahoma, Arial, Verdana;
}

html {
    height: 100%;
    overflow: visible;
    font: 8pt Tahoma, Arial, Verdana;
}

.mstrmojo-overlayBox {
	position: fixed;
    top: 0px;
    left: 0px;
    width: 100%;
    height: 100%;
    background-color: white;
    opacity: 0.3;
    z-index: 499;
}

.mstrmojo-OverlayLoading {
	position: relative;
	background: url(images/IPA/big-loading.gif) no-repeat;
	font: bold 14pt Tahoma, Arial, Verdana;
    top: 30%;
    left: 30%;
    height: 300px;
    width: 300px;
    color: black;
}


