    margin-bottom: 5px;
    border: solid 1px #AAA;
    overflow: visible;
    -moz-border-radius: 3px;
    border-radius: 5px;
}

.mstrmojo-EditableLabel {
	cursor: pointer;
}

.mstrmojo-PanelButton {
