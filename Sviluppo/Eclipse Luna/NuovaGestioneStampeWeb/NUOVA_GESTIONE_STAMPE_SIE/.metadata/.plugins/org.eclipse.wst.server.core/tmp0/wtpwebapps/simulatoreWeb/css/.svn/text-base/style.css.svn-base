/*la classe di stile hidden è utilizzata solo per le
//etichette nascoste per i campi disabilitati dopo una ricerca "..."
*/
.hidden {
	position: absolute;
	left: 0px;
	top: -500px;
	width: 1px;
	height: 1px;
	overflow: hidden;
}

